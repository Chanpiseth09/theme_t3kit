[globalVar = LIT:0 = {$themes.configuration.feLayoutCssFileExtension}] && [globalVar = LIT:small = {$themes.configuration.feLayoutMode}]

[global]

[globalVar = LIT: = {$themes.configuration.feLayoutCssFileExtension}] && [globalVar = LIT:medium = {$themes.configuration.feLayoutMode}]

[global]

[globalVar = LIT: = {$themes.configuration.feLayoutCssFileExtension}] && [globalVar = LIT:big = {$themes.configuration.feLayoutMode}]
    includeCSS {
        bootstrap = {$themes.configuration.felayoutFullPath}/bootstrap.css
        components = {$themes.configuration.felayoutFullPath}/components.css
        header = {$themes.configuration.felayoutFullPath}/{themes.configuration.headerType}.css
        maincss = {$themes.configuration.felayoutFullPath}/main.css
    }
[global]

