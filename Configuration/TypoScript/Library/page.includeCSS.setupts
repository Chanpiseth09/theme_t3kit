page {
    includeCSS {
        bootstrap = {$themes.configuration.felayoutFullPath}/bootstrap.less
        bootstrap {
            excludeFromConcatenation = 1
            disableCompression = 1
        }
        components = {$themes.configuration.felayoutFullPath}/components.css
        components {
            excludeFromConcatenation = 1
            disableCompression = 1
        }
        header = {$themes.configuration.felayoutFullPath}/{$themes.configuration.headerLayout}.less
        header {
            excludeFromConcatenation = 1
            disableCompression = 1
        }
        maincss = {$themes.configuration.felayoutFullPath}/main.less
        maincss {
            excludeFromConcatenation = 1
            disableCompression = 1
        }
    }
